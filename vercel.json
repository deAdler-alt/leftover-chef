{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/", "destination": "/api" },
    { "source": "/plan", "destination": "/api/plan" },
    { "source": "/row", "destination": "/api/row" },
    { "source": "/static/:path*", "destination": "/api/static/:path*" }
  ]
}

