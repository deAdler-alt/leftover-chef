{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "rewrites": [
    { "source": "/api/suggest-ai", "destination": "/api/suggest-ai.js" },
    { "source": "/api/extract", "destination": "/api/extract.js" },
    { "source": "/static/(.*)", "destination": "/api/index.py" },
    { "source": "/(.*)", "destination": "/api/index.py" }
  ]
}
