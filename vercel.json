{
  "functions": {
    "api/index.py": { "runtime": "python3.11" },
    "api/*.js": { "runtime": "nodejs20.x" }
  },
  "routes": [
    { "src": "^/api/suggest-ai$", "dest": "/api/suggest-ai.js" },
    { "src": "^/api/extract$", "dest": "/api/extract.js" },
    { "src": "^/static/(.*)$", "dest": "/api/index.py" },
    { "src": "^(?!/api/).*", "dest": "/api/index.py" }
  ]
}
